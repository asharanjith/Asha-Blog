<p style="color: green"><%= notice %></p>

<h4>
  <%= link_to "Sign out", destroy_user_session_path, method: :delete %>
</h4>

<h1>Posts</h1>

<div id="posts">
  <% @posts.each do |post| %>
    <%= render post %>
    <p>
      <%= link_to "Show this post", post %>
    </p>
  <% end %>
</div>

<%= link_to "New post", new_post_path %>
